{
  "name": "travel-booking-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Frontend application for Travel Booking System - A modern React-based booking platform.",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 5174",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,md}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "clean": "rm -rf node_modules package-lock.json dist"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.26.2",
    "react-hot-toast": "^2.4.1",
    "react-hook-form": "^7.48.2",
    "react-query": "^3.39.3",
    "date-fns": "^2.30.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.294.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.47",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.4.13",
    "vite": "^5.4.8",
    "vitest": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "@vitest/coverage-v8": "^1.0.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "react",
    "vite",
    "travel",
    "booking",
    "frontend",
    "tailwindcss",
    "javascript"
  ],
  "author": "Travel Booking System Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/travel-booking-system.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/travel-booking-system/issues"
  },
  "homepage": "https://github.com/your-username/travel-booking-system#readme"
}

